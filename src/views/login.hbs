<form method="POST" action="/api/sessions/login" id="login-form">
  <label for="email">email:</label>
  <input type="email" name="email" id="email" required />
  <label for="password">password:</label>
  <input type="password" name="password" required />
  <button type="submit">Login</button>
</form>
<div>
    <a href="/api/sessions/auth/github"><button>Login with GitHub.</button></a>
    <a href="/register"><button>Do not have an account? Sign up.</button></a>
    <a href="/changepass">Forget my password.</a>
</div>